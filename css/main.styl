
@import vars

body,html
  padding 0
  margin 0

ul,li
  margin 0
  padding 0
  list-style-type none

.clear
  clear both
  display block
  overflow hidden
  visibility hidden
  width 0
  height 0

.fade
  display none
  opacity 0
  position absolute
  top 0px
  left 0px
  width 100%
  height 100%
  background url('/img/opacity/blue_90.png')
